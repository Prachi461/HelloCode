package Array;

public class AnimalArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

        // Create an array of 10 animals
        String[] animals = {"Lion", "Elephant", "Tiger", "Giraffe", "Zebra", "Penguin", "Kangaroo", "Panda", "Dolphin", "Eagle"};

        // Print the array
        for (String animal : animals) {
            System.out.println(animal);
        }

	}

}
