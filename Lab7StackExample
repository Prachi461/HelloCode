public class Lab7StackExample{
	public static void main(String[] args) {
		Stack<Integer> stack = new Stack<>();
		for (int i=1; i<=10; i++) stack.push(i);
		for (int i=1; i<5; i++)stack.pop();
		System.out.println(stack);
	}
}

